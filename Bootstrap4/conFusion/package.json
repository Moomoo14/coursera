{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "clean": "rimraf distribution",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* distribution/fonts",
    "imagemin": "imagemin img/* -o distribution/img",
    "usemin": "usemin contactus.html -d distribution --htmlmin -o distribution/contactus.html && usemin aboutus.html -d distribution --htmlmin -o distribution/aboutus.html && usemin index.html -d distribution --htmlmin -o distribution/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "clean-css": "^4.2.1",
    "copyfiles": "^2.1.0",
    "cssmin": "^0.4.3",
    "del": "^3.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-sass": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-rev": "^8.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-usemin": "^0.3.29",
    "htmlmin": "0.0.7",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.3",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.0",
    "popper.js": "^1.15.0",
    "tar": "^4.4.8",
    "uglify-js": "^3.5.4"
  }
}
